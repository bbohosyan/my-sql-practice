Link: https://www.w3schools.com/sql/exercise.asp
1. SELECT * FROM Customers;
2. SELECT City FROM Customers;
3. SELECT DISTINCT Country FROM Customers;
4. SELECT * FROM Customers WHERE City = 'Berlin';
5. SELECT * FROM Customers WHERE NOT City = 'Berlin';
6. SELECT * FROM Customers WHERE CustomerID = 32;
7. SELECT * FROM Customers WHERE City = 'Berlin'AND PostalCode = 12209;
8. SELECT * FROM Customers WHERE City = 'Berlin'OR City = 'London';
9. SELECT * FROM Customers ORDER BY City;
10. SELECT * FROM Customers ORDER BY City DESC;
11. SELECT * FROM Customers ORDER BY Country, City;
12. INSERT INTO Customers (CustomerName, Address, City, PostalCode,Country) VALUES ('Hekkan Burger','Gateveien 15','Sandnes','4306','Norway');
13. SELECT * FROM Customers WHERE PostalCode IS NULL;
14. SELECT * FROM Customers WHERE PostalCode IS NOT NULL;
15. UPDATE Customers SET City = 'Oslo';
16. UPDATE Customers SET City = 'Oslo'WHERE Country = 'Norway';
17. UPDATE Customers SET City = 'Oslo',Country = 'Norway' WHERE CustomerID = 32;
18. DELETE FROM Customers WHERE Country = 'Norway';
19. DELETE FROM Customers;
20. SELECT MIN(Price) FROM Products;
21. SELECT MAX(Price) FROM Products;
22. SELECT COUNT(*) FROM Products WHERE Price = 18;
23. SELECT AVG(Price) FROM Products;
24. SELECT SUM(Price) FROM Products;
25. SELECT * FROM Customers WHERE City LIKE 'a%';
26. SELECT * FROM Customers WHERE City LIKE '%a';
27. SELECT * FROM Customers WHERE City LIKE '%a%';
28. SELECT * FROM Customers WHERE City LIKE 'a%b';
29. SELECT * FROM Customers WHERE City NOT LIKE 'a%';
30. SELECT * FROM Customers WHERE City LIKE '_a%';
31. SELECT * FROM Customers WHERE City LIKE '[acs]%';
32. SELECT * FROM Customers WHERE City LIKE '[a-f]%';
33. SELECT * FROM Customers WHERE City LIKE '[^acf]%';
34. SELECT * FROM Customers WHERE Country IN ('Norway', 'France');
35. SELECT * FROM Customers WHERE Country NOT IN ('Norway', 'France');
36. SELECT * FROM Products WHERE Price BETWEEN 10 AND 20;
37. SELECT * FROM Products WHERE Price NOT BETWEEN 10 AND 20;
38. SELECT * FROM Products WHERE ProductName BETWEEN 'Geitost' AND 'Pavlova';
39. SELECT CustomerName,Address,PostalCode AS Pno FROM Customers;
40. SELECT * FROM Customers AS Consumers;
41. SELECT *FROM Orders LEFT JOIN Customers ON Orders.CustomerID=Customers.CustomerID;
42. SELECT *FROM Orders INNER JOIN Customers ON Orders.CustomerID=Customers.CustomerID;
43. SELECT *FROM OrdersRIGHT JOIN Customers ON Orders.CustomerID=Customers.CustomerID;
44. SELECT COUNT(CustomerID),Country FROM Customers GROUP BY Country;
45. SELECT COUNT(CustomerID),Country FROM Customers GROUP BY Country ORDER BY COUNT(CustomerID) DESC;
46. CREATE DATABASE testDB;
47. DROP DATABASE testDB;
48. CREATE TABLE Persons (  PersonID int,  LastName varchar(255),  FirstName varchar(255),  Address varchar(255),  City varchar(255) );
49. DROP TABLE Persons;
50. TRUNCATE TABLE Persons;
51. ALTER TABLE PersonsADD Birthday DATE;
52. ALTER TABLE PersonsDROP COLUMN Birthday;
